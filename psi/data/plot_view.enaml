from chaco.api import OverlayPlotContainer
from enaml.widgets.api import Container, DockItem
from traits_enaml.widgets.enable_canvas import EnableCanvas

enamldef ChacoDockItem(DockItem):

    closable = False
    attr plugin
    attr container_name

    Container:
        EnableCanvas:
            component << plugin._containers.get(container_name,
                                                OverlayPlotContainer())
